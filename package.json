{
  "name": "local-solutions-designer",
  "author": "Colton McClintock",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.17.1 <19.0.0"
  },
  "description": "An Express.js application with SPFx webpart and UI component generation",
  "main": "dist/server/server.js",
  "scripts": {
    "start": "node dist/server/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only server/server.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "prestart": "npm run build"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "npm": "^10.8.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "typescript": "4.7.4"
  }
}
